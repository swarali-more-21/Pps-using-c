#include<stdio.h>
int main()
{
int marks;

printf("Enter the Marks of Student:\t");

scanf("%d", &marks);

if(marks>100)

{
 printf("Invalid");
}

else if (marks>=90)
{
 printf("A Grade");
}

else if (marks>=75)
{
 printf("B Grade");
}

else if (marks>=60)
{
 printf("C Grade");
}
else if (marks>=40)
{
 printf("D Grade");
}

else if(marks<0)
{
 printf("Invalid");
}
else
{
 printf("Fail");
 }
 
 return 0;
}
