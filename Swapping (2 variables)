#include<stdio.h>
int main() 
{
    int a,b;
    printf("Enter the variable A \n");
    scanf("%d", &a);
    printf("Enter the variable B \n");
    scanf("%d", &b);
    printf("Before swapping A=%d and b=%d \v", a , b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("After Swapping A=%d and B=%d", a , b);

    return 0;
}
