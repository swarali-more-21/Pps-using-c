#include<stdio.h>

int main()
 {
    int num1, num2 , num3, avg;
    printf("Enter First Number:");
    scanf("%d", &num1);
    printf("Enter Second Number:");
    scanf("%d", &num2);
    printf("Enter Third Number:");
    scanf("%d", &num3);
    avg=(num1+num2+num3)/3;
    printf("Average of %d, %d,%d is %d", num1,num2,num3,avg);
    
    
    return 0;
 }
